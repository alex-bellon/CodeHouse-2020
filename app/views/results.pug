extends layout

block content
  div(class='table')
    table
      each item in myList
        tr
          div(class='rows')
            - var url = '/bizInfo/?id=' + item.ID;
            a(href=url) #{item.Name}
        tr
                td="Location: " + item.Address 
        tr
                td="Tel: "+ item.Phone
        tr
                td="Info: " + item.Description
        tr
            img(src=item.Images)    